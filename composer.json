{
    "name": "tiagoaragona/echosbot",
    "description": "Packages to be installed to support Gemini",
    "type": "library",
    "require": {
        "gemini-api-php/client": "^1.4",
        "symfony/http-client": "^7.0",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Tiagoaragona\\Echosbot\\": "src/"
        }
    },
    "authors": [
        {
            "name": "tiagoaragona",
            "email": "tiagoaragona@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
